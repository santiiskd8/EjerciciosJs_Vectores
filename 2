// Que reciba un array de 5 dias y devuelva true o false si algun dia del array es fin de semana.
let a;

function finde(vector){
  for(let i= 0; i < 5;i++){
    a = vector[i].toLowerCase();
    if(a == "sabado" || a == "domingo"){
      return false;
    }
  }
  return true;
}
finde(["lunes","martes","miercoles","jueves","Domingo"]);
